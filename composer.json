{
    "name": "sandermangel/magesdk",
    "description": "A PHP based SDK for Magento 2.x",
    "type": "library",
    "require": {
        "php": "^7.0",
        "vlucas/phpdotenv": "^2.4",
        "symfony/console": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "friendsofphp/php-cs-fixer": "^2.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sander Mangel",
            "email": "sander@sandermangel.nl"
        }
    ],
    "autoload": {
      "psr-4": {
        "Sandermangel\\MageSDK\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\MageSDK\\": "tests/Unit/",
        "Integrations\\MageSDK\\": "tests/Integration/"
      }
    }
}
